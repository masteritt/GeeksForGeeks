#include <iostream>
using namespace std;

#define MAX 11
int lcs[MAX][MAX];
int M,N;
char a[MAX];
char b[MAX];

int max(int a, int b){
	return a > b ? a : b;
}
int computeLcs(){
	if ((M == 0) || (N == 0))
		return 0;

	for (int i = 0; i <= M; i++)
		for (int j = 0; j <= N; j++)
		{
			if ((i == 0) || (j == 0))
				lcs[i][j] = 0;
			else if (a[i] == b[j])
				lcs[i][j] = lcs[i - 1][j - 1] + 1;
			else
				lcs[i][j] = max(lcs[i - 1][j], lcs[i][j - 1]);
		}
	return lcs[M][N];
}
int main(){
	//freopen("input.txt", "r", stdin);
	cin >> M>>N;
	for (int i = 1; i <= M; i++)
		cin >> a[i];
	for (int j = 1; j <= N; j++)
		cin >> b[j];

	cout << computeLcs() << endl;
	return 0;
}
