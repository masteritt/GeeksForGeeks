#include <iostream>
using namespace std;
#define MAX 10
int S[MAX];
int N,M;
int dp[MAX][MAX+1];

int count(){
	for (int i = 0; i < M; i++)
		dp[i][0] = 1;
	for (int i = 0; i < M; i++)
		for (int j = 1; j < (N + 1); j++)
		{
			int x, y;
			if (j>=S[i])
				x = dp[i][j - S[i]];//coin step back
			else
				x = 0;
			if (i >= 1)
				y = dp[i - 1][j];//up value
			else
				y = 0;
			dp[i][j] = x + y;
		}
	return dp[M-1][N];
}

int main(){
	cin >> N>>M;
	for (int i = 0; i < M; i++)
		cin >> S[i];
	cout << count() << endl;

	return 0;
}
